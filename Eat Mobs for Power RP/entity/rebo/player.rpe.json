      "scripts": {
        "initialize": [
          "v.health = q.health;",
          "v.swim_amount = 0.0;",
          "v.is_brandishing_spear = 0.0;",
          "v.is_holding_spyglass = 0.0;",
          "v.ischarging = 0.0;",
          "v.charging_start = 0.0;",
          "v.time_elapsed = 0.0;",
          "v.is_doing_custom_glide = 0.0;",
          "v.can_sleep_animation = 0.0;",
          "v.can_swim_animation = 0.0;",

          "v.is_holding_right = 0.0;",
          "v.is_blinking = 0.0;",
          "v.last_blink_time = 0.0;",
          "v.hand_bob = 0.0;"
        ],
        
        "pre_animation": [
          // Default
            "v.time_elapsed = v.time_elapsed + q.delta_time;",
            // "v.riding_offset = q.variant == 1 ? 12.0;",
            // "v.riding_offset = q.variant == 3 ? 100.0;",
            // "v.riding_offset = q.variant == 4 ? 21.0;",

            // "v.modified_distance_moved = q.is_ignited ? ((q.life_time - math.round(q.life_time)) * q.is_moving) + q.ground_speed * 0.25 : q.modified_distance_moved;",
            // "v.modified_move_speed = q.is_ignited ? ((q.life_time - math.round(q.life_time)) * q.is_moving) + q.ground_speed * 0.35 : q.modified_move_speed;",

            // "v.modified_distance_moved = q.is_ignited ? q.ground_speed - math.round(q.ground_speed) : q.modified_distance_moved;",
            // "v.modified_move_speed = q.is_ignited ?  q.ground_speed - math.round(q.ground_speed) : q.modified_move_speed;",

            "v.modified_distance_moved = q.is_ignited ? math.cos(q.anim_time * 360) * q.is_moving : q.modified_distance_moved;",
            "v.modified_move_speed = q.is_ignited ?  math.cos(q.anim_time * 360) * q.is_moving : q.modified_move_speed;",
            
            "v.helmet_layer_visible = 1.0;",
            "v.leg_layer_visible = 1.0;",
            "v.boot_layer_visible = 1.0;",
            "v.chest_layer_visible = 1.0;",
            "v.attack_body_rot_y = Math.sin(360*Math.sqrt(v.attack_time)) * 5.0;",

            "v.can_sleep_animation = q.variant == 0 || q.variant == 2 || q.variant == 5 || q.variant == 6 || q.variant == 13|| q.variant == 15 || q.variant == 17 || q.variant == 18;",
            "v.can_swim_animation = q.variant == 0 || q.variant == 2 || q.variant == 5 || q.variant == 6 || q.variant == 13|| q.variant == 15 || q.variant == 18;",
            
            "v.is_doing_custom_glide = q.variant == 1 || q.variant == 3 || q.variant == 4 || q.variant == 7|| q.variant == 8 || q.variant == 10 || q.variant == 12 || q.variant == 14 || q.variant == 16;",

            // "v.swim_amount = !v.can_sleep_animation ? 0.0;",
            // "v.gliding_speed_value = !v.can_sleep_animation ? 0.0;",

            "v.tcos0 = (math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed /  v.gliding_speed_value) * 57.3;",
            "v.first_person_rotation_factor = math.sin((1 - v.attack_time) * 180.0);",
            "v.hand_bob = q.life_time < 0.01 ? 0.0 : v.hand_bob + ((q.is_on_ground && q.is_alive ? math.clamp(math.sqrt(math.pow(q.position_delta(0), 2.0) + math.pow(q.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - v.hand_bob) * 0.02;",

            "v.map_angle = math.clamp(1 - v.player_x_rotation / 45.1, 0.0, 1.0);",
            "v.item_use_normalized = q.main_hand_item_use_duration / q.main_hand_item_max_duration;",

          // Axolotl
            "v.moving = q.ground_speed > 0 || q.vertical_speed > 0;",
            "v.pitch = 0;",

           // Chicken
            "v.chicken_wing_flap = ((math.cos((q.life_time * !(q.is_on_ground || q.is_riding)) * 1000) * -45) + 45);",

          // Creeper
            "v.exploding = v.exploding ?? 0;",
            "v.exploding = !q.is_illager_captain ? 0 : v.exploding + (q.is_illager_captain * 0.01);",
            "v.wobble = Math.sin(v.exploding * 5730) * v.exploding * 0.01 + 1.0;",
            "v.swelling_scale1 = (Math.pow(Math.clamp(v.exploding, 0.0, 1.0), 4.0) * 0.4 + 1.0) * v.wobble;",
            "v.swelling_scale2 = (Math.pow(Math.clamp(v.exploding, 0.0, 1.0), 4.0) * 0.1 + 1.0) / v.wobble;",
            "v.leg_rot = Math.cos(q.modified_distance_moved * 38.17326) * 80.22 * q.modified_move_speed;",
            "v.flash = Math.mod(Math.Round(v.exploding * 5.0), 2.0);",

          // Frog
            "v.croak = math.mod(math.round(q.life_time), 10) == 0;",
          
          // Ghast
            "v.ischarging = v.charging_start + 1 > q.life_time;",
          
          // Parrot
            "v.state = q.is_dancing ? 3 : (q.is_sitting ? 2 : (!q.is_on_ground && !q.is_riding ? 0 : 1));",
            "v.dance.x = Math.cos(q.life_time * 57.3 * 20.0);",
            "v.dance.y = -Math.sin(q.life_time * 57.3 * 20.0);",
            "v.parrot_wing_flap = math.sin(q.life_time * 1500) + 45;",

          // Salmon
            "v.zrot = !q.is_in_water ? Math.cos((q.time_stamp + q.frame_alpha) * 14.32) * 90 : 0.0;",
            // "v.AnimationAmountBlend = Math.lerp(v.AnimationAmountPrev, v.AnimationAmount, q.frame_alpha);",
            "v.wiggle_speed = q.life_time * 20 * q.is_moving;",
        
          // Strider
            "v.animation_speed = Math.min(q.modified_move_speed, 0.25);",
            "v.speed = 85.9437;",
            "v.bristle_flow = Math.cos(q.modified_distance_moved * v.speed + 180) * v.animation_speed;",
            "v.bristle_range_mod = 1;",
            "v.bristle_speed_mod = 1;"
        ],
        
        "animate": [
            {"custom_gliding": "(q.variant == 1 || q.variant == 3 || q.variant == 4 || q.variant == 7 || q.variant == 10 || q.variant == 12 || q.variant == 14 || q.variant == 16) && !v.is_first_person && q.is_gliding"},
