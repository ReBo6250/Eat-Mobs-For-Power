                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "pig" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "piglin_brute" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "player" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "salmon" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "skeleton" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "spider" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "zombie" }
                  ]
                },
                { "test": "in_lava", "subject": "self", "operator": "equals", "value": true }
              ]
            },
            "cause": "lava",
            "damage_per_tick": 4
          },
          {
            "filters": {
              "all_of": [
                { 
                  "any_of": [
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "blaze" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "enderman" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "strider" }
