  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:player",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false,
      "animations": {
        "ac.item.bee": "controller.animation.player.item.bee",
        "ac.jump": "controller.animation.player.jump",
        
        "stunned": "controller.animation.player.stunned",
        "is_riding": "controller.animation.player.is_riding",
        "in_water": "controller.animation.player.in_water",
        "jumping": "controller.animation.player.jumping",
        "on_attack": "controller.animation.player.on_attack",
        "on_ground": "controller.animation.player.on_ground",
        "on_jump": "controller.animation.player.on_jump",
        "sneaking": "controller.animation.player.sneaking",
        "sprinting": "controller.animation.player.sprinting",
        "variant": "controller.animation.player.variant"
      },
      "scripts": {
        "animate": [
          "ac.item.bee",
          "ac.jump",
          "stunned",
          "is_riding",
          "in_water",
          "jumping",
          "on_attack",
          "on_ground",
          "on_jump",
          "sneaking",
          "sprinting",
          "variant"
        ]
      }
    },


    "components": {

      "minecraft:environment_sensor": {
        "triggers": [
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "self",
                  "value": "player"
                },
                {
                  "test": "has_mob_effect",
                  "subject": "self",
                  "value": "bad_omen"
                },
                {
                  "test": "is_in_village",
                  "subject": "self",
                  "value": true
                }
              ]
            },
            "event": "minecraft:trigger_raid"
          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "self",
                  "value": "creeper"
                },
                {
                    "test": "has_component",
                    "subject": "self",
                    "operator": "not",
                    "value": "minecraft:is_sheared"
                },
                {
                  "test": "has_component",
                  "subject": "self",
                  "operator": "not",
                  "value": "minecraft:is_illager_captain"
                },
                {
                  "test": "is_sneaking",
                  "subject": "self",
                  "value": true
                }
              ]
            },
            "event": "e:add-exploding"
          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "self",
                  "value": "creeper"
                },
                {
                  "test": "is_sneaking",
                  "subject": "self",
                  "value": false
                }
              ]
            },
            "event": "e:remove-exploding"
          },
          {
            "filters": {
              "all_of": [
                { "test": "is_family", "subject": "self", "value": "strider" },
                {
                  "any_of": [
                    { "test": "in_lava", "subject": "self", "value": true },
                    { "test": "has_biome_tag", "subject": "self", "operator": "equals", "value": "nether" }
                  ]
                }
              ]
            },
            "event": "e:add-ignited"
          },
          {
            "filters": {
              "all_of": [
                { "test": "is_family", "subject": "self", "value": "strider" },
                { "test": "in_lava", "subject": "self", "value": false },
                { "test": "has_biome_tag", "subject": "self", "operator": "not", "value": "nether" }
              ]
            },
            "event": "e:remove-ignited"
          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "has_biome_tag",
                  "subject": "self",
                  "operator": "equals",
                  "value": "nether"
                }
              ]
            },
            "event": "e:add-stunned"
          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "has_biome_tag",
                  "subject": "self",
                  "operator": "not",
                  "value": "nether"
                }
              ]
            },
            "event": "e:remove-stunned"
          }
        ]
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "lightning",
            "on_damage": {
              "filters": [
                { "test": "is_family", "subject": "self", "value": "axolotl" }
              ]
            },
            "deals_damage": true,
            "damage_multiplier": 2000.0
          },
          {
            "on_damage": {
              "filters": [
                {
                  "any_of": [
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "blaze" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "cat" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "chicken" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "enderman" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "ghast" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "parrot" }
                  ]
                }
              ]
            },
            "cause": "fall",
            "deals_damage": false
          },
          {
            "on_damage": {
              "filters": [
                {
                  "none_of": [
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "blaze" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "cat" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "chicken" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "enderman" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "ghast" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "parrot" }
                  ]
                }
              ]
            },
            "cause": "fall",
            "deals_damage": true
          },
          {
            "on_damage": {
              "filters": {
                "all_of": [
                    { "test": "is_family", "subject": "self", "value": "fox" },
                    { "test": "is_block", "subject": "block", "value": "minecraft:sweet_berry_bush" }
                ]
              }
            },
            "deals_damage": false
          },
          {
            "cause": "suffocation",
            "on_damage": {
              "filters": {
                "any_of": [
                  [
                    { "test": "is_family", "subject": "self", "value": "axolotl" },
                    { "test": "is_family", "subject": "self", "value": "cat" },
                    { "test": "is_family", "subject": "self", "value": "chicken" },
                    { "test": "is_family", "subject": "self", "value": "fox" },
                    { "test": "is_family", "subject": "self", "value": "frog" },
                    { "test": "is_family", "subject": "self", "value": "parrot" },
                    { "test": "is_family", "subject": "self", "value": "pig" }
                  ]
                ]
              }
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "all_of": [
                { 
                  "any_of": [
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "axolotl" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "cat" },
                    { "test": "is_family", "subject": "self", "operator": "equals", "value": "chicken" },
